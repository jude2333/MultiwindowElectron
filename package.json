{
  "name": "multiwindow",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . electron-launcher --platform=win32 --arch=x64 --out=dist --overwrite",
    "dist:mac": "npx electron-builder --mac"
  },
  "build": {
    "appId": "com.yourcompany.multiwindow",
    "productName": "MultiWindow",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": "dmg"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "electron": "36.3.1",
    "electron-builder": "24.13.3",
    "electron-packager": "17.1.2"
  },
  "author": "Your Name",
  "license": "ISC",
  "description": "Electron multi-window application"
}