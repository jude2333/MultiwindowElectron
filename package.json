{
  "name": "multiwindow",
  "version": "1.0.0",
  "main": "main.js",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.example.multiwindow",
    "productName": "Multiwindow",
    "files": [
      "**/*",
      "!node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/.bin",
      "!**/*.{map,md,ts}",
      "!dist"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    }
  },
  "devDependencies": {
    "electron": "^36.3.1",
    "electron-builder": "^24.13.3"
  }
}
